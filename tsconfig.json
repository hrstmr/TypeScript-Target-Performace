{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "ES2015",
        // "target": "ES2022",
        "lib": ["ES2016", "ES2018", "ES2019", "ES2020", "ES2021", "ES2022", "dom"],
        // "module": "CommonJS",
        "module": "ESNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "skipLibCheck": true,
        "downlevelIteration": true
    }
}
